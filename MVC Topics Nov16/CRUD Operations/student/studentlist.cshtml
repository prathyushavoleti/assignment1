@model List<CRUD_Operations.context.student>
@{
    ViewBag.Title = "studentlist";
}
<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Description</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Name</td>
        <td>@item.Email</td>
        <td>@item.Mobile</td>
        <td>@item.Description</td>
        <td>
            <a href="@Url.Action("student",new {@item.ID,item.Name,item.Mobile,item.Email,item.Description})" class="btn btn-success">Edit</a>
            <a href="@Url.Action("Delete",new {@item.ID})" class="btn btn-danger">Delete</a>

        </td>

    </tr>
    }


</table>


